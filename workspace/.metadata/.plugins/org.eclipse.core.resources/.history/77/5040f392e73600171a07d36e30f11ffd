w=['airline']
n=8
r=[]
l=[]
l=len(w)
c=8
for i in range (n):
    g=input("ENTER THE GUESSES")
    c=c-1
    for j in range (l):
        if(g==w[j]):
            r.append (g)
        else:
            l.append(g)
    for r in range(w):
        for k in range(r):
            if(r[k]==w[j]):
               s=r[k]
               print("%d"%s)
            print("%d"%c) 
            

    
     
                
       
        
        
    
    
    
    
    
